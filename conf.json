{
    "title":"UGSM code documentation",
    "recurseDepth":2,
    "opts":{
        "title":"UGSM code documentation",
        "recurse":true,
        "destination":"./jsdoc",
        "readme":"./README.md"
    },
    "source": {
        "include": ["src/public/javascript/classes","src/public/javascript/custom_modules"],
        "exclude": ["src/public/javascript/renderers","./src/main.js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    }
}